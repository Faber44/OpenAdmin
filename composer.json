{
    "name": "Faber44/open-admin-media-library",
    "description": "Media-library extension for Laravel Open-Admin",
    "type": "library",
    "keywords": ["Open-Admin", "extension", "media-library", "media", "Spatie"],
    "homepage": "https://github.com/Faber44/open-admin-media-library",
    "license": "MIT",
    "authors": [
        {
            "name": "Faber",
            "email": "faber44@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Faber44/open-admin-media-library.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "open-admin-org/open-admin": "^1.0",
        "spatie/laravel-medialibrary": "^10.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Faber44\\OpenAdmin\\MediaLibrary\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Faber44\\OpenAdmin\\MediaLibrary\\MediaLibraryServiceProvider"
            ]

        }
    }
}
